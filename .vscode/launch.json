{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Open Old Version",
            "file": "${workspaceFolder}/old_src/index.html",
            "userDataDir": "${workspaceFolder}/.vscode/chrome-debug-profile",
            "runtimeArgs": ["--allow-file-access", "--disable-web-security"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Vite",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "dev"],
            "skipFiles": ["<node_internals>/**"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/react_version"
        }
    ]
}
