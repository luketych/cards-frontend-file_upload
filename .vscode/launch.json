{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Open Old Version",
            "file": "${workspaceFolder}/old_src/index.html",
            "userDataDir": "${workspaceFolder}/.vscode/chrome-debug-profile",
            "runtimeArgs": ["--allow-file-access", "--disable-web-security"]
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Open React Version",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/react_version",
            "userDataDir": "${workspaceFolder}/.vscode/chrome-debug-profile",
            "preLaunchTask": "Start React Dev Server"
        }
    ]
}
